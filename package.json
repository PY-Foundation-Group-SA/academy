{
  "name": "angular-website-template",
  "version": "0.0.1",
  "scripts": {
    "ng": "npx ng",
    "start": "npx ng serve",
    "build": "npx ng build",
    "test": "npx ng test",
    "lint": "npx ng lint",
    "e2e": "npx ng e2e",
    "dev:ssr": "npx ng run angular-website-template:serve-ssr",
    "serve:ssr": "npx node dist/angular-website-template/server/main.js",
    "build:ssr": "npx ng build --prod && npx ng run angular-website-template:server:production",
    "prerender": "npx ng run angular-website-template:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.2",
    "@angular/common": "~10.0.2",
    "@angular/compiler": "~10.0.2",
    "@angular/core": "~10.0.2",
    "@angular/forms": "~10.0.2",
    "@angular/platform-browser": "~10.0.2",
    "@angular/platform-browser-dynamic": "~10.0.2",
    "@angular/platform-server": "~10.0.2",
    "@angular/router": "~10.0.2",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@nguniversal/express-engine": "^10.0.1",
    "bootstrap": "^4.5.0",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "nodemailer": "^6.4.10",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@lenne.tech/npm-package-helper": "0.0.10",
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular/cli": "~10.0.1",
    "@angular/compiler-cli": "~10.0.2",
    "@angular/language-service": "~10.0.2",
    "@nguniversal/builders": "^10.0.1",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "@types/jasmine": "~3.5.11",
    "@types/jasminewd2": "~2.0.8",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "typescript": "~3.9.6",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "husky": "4.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ts-node --skip-project extras/sync-version.ts && pretty-quick --staged",
      "pre-push": "npm run lint"
    }
  }
}
