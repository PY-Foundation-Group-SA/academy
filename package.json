{
  "name": "acaedmy",
  "version": "0.0.1",
  "scripts": {
    "ng": "npx ng",
    "start": "npx ng serve",
    "build": "npx ng build",
    "test": "npx ng test",
    "lint": "npx ng lint",
    "e2e": "npx ng e2e",
    "dev:ssr": "npx ng run acaedmy:serve-ssr",
