{
  "name": "angular-website-template",
  "version": "0.0.1",
  "scripts": {
    "ng": "npx ng",
    "start": "npx ng serve",
    "build": "npx ng build",
    "test": "npx ng test",
    "lint": "npx ng lint",
    "e2e": "npx ng e2e",
    "dev:ssr": "npx ng run angular-website-template:serve-ssr",
    "serve:ssr": "npx node dist/angular-website-template/server/main.js",
    "build:ssr": "npx ng build --prod && npx ng run angular-website-template:server:production",
    "prerender": "npx ng run angular-website-template:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.3",
    "@angular/common": "~11.0.3",
    "@angular/compiler": "~11.0.3",
    "@angular/core": "~11.0.3",
    "@angular/forms": "~11.0.3",
    "@angular/platform-browser": "~11.0.3",
    "@angular/platform-browser-dynamic": "~11.0.3",
    "@angular/platform-server": "~11.0.3",
    "@angular/router": "~11.0.3",
    "@nguniversal/express-engine": "^10.1.0",
    "bootstrap": "^4.5.3",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "nodemailer": "^6.4.16",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.3",
    "@angular/cli": "~11.0.3",
    "@angular/compiler-cli": "~11.0.3",
    "@angular/language-service": "~11.0.3",
    "@lenne.tech/npm-package-helper": "0.0.10",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.11",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "codelyzer": "^6.0.1",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "husky": "4.2.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.10.2",
    "tslint": "^6.1.3",
    "typescript": "~4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ts-node --skip-project extras/sync-version.ts && pretty-quick --staged",
      "pre-push": "npm run lint"
    }
  }
}
